{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
          "es2015",
          "dom"
    ],
    "sourceMap": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": [
      "./*.ts"
  ]
}