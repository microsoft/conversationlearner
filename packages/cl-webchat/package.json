{
  "name": "@conversationlearner/webchat",
  "version": "0.0.0-development",
  "description": "Embeddable web chat control for the Conversation Learner (fork of Microsoft/BotFramework-WebChat)",
  "main": "built/BotChat.js",
  "types": "built/BotChat.d.ts",
  "scripts": {
    "build-test": "tsc -p ./test/mock_dl && tsc -p ./test && tsc -p ./test/mock_speech",
    "build-test-watch": "concurrently \"tsc -w -p ./test/mock_dl\" \"tsc -w -p ./test\"",
    "build-css": "node-sass ./src/scss/ -o . && rimraf ./includes/",
    "build-ac-config": "ac-scss2json ./src/scss/includes/adaptive-card-config.scss > adaptivecards-hostconfig.json",
    "build-all-style": "npm run build-css && npm run build-ac-config && webpack",
    "build": "npm run build-css && tsc && webpack --progress",
    "watch": "npm-run-all -p -r -l tsc-watch webpack-watch",
    "tsc-watch": "tsc -w",
    "webpack": "webpack",
    "webpack-watch": "webpack --progress --watch",
    "clean": "rimraf built",
    "prepublish": "tsc && npm run build-css && webpack --progress --config webpack.production.config.js",
    "start": "http-server -p 8000",
    "test": "concurrently  \"node test/mock_dl/index.js\" \"mocha test\" ",
    "mock": "node test/mock_dl/index.js",
    "setpackageversion": "tsc -p ./scripts/tsconfig.json && node ./scripts/setpackageversion.js"
  },
  "repository": {
    "type": "git",
    "url": "git://git@github.com/Microsoft/ConversationLearner-WebChat.git"
  },
  "author": "Microsoft Corp",
  "license": "MIT",
  "dependencies": {
    "botframework-directlinejs": "^0.9.12",
    "core-js": "^2.5.1",
    "markdown-it": "^8.4.0",
    "adaptivecards": "1.2.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.1.0",
    "redux": "^3.7.2",
    "redux-observable": "^0.17.0",
    "rxjs": "^5.4.3",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.33",
    "@types/chai": "3.4.34",
    "@types/chai-subset": "1.0.29",
    "@types/deep-freeze": "0.0.29",
    "@types/execa": "^0.8.1",
    "@types/express": "4.0.35",
    "@types/fs-extra": "^5.0.0",
    "@types/markdown-it": "0.0.2",
    "@types/nightmare": "1.6.30",
    "@types/react": "16.0.34",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.21",
    "awesome-typescript-loader": "3.1.2",
    "body-parser": "^1.19.0",
    "chai": "3.5",
    "chai-subset": "1.4.0",
    "concurrently": "3.1.0",
    "deep-freeze": "0.0.1",
    "dotenv": "4.0.0",
    "execa": "^0.9.0",
    "expose-loader": "0.7.3",
    "express": "^4.17.1",
    "fs-extra": "^5.0.0",
    "http-server": "^0.11.1",
    "mocha": "3.2.0",
    "nightmare": "2.8.1",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "3.1.2",
    "remote": "0.2.6",
    "rimraf": "2.6.1",
    "source-map-loader": "0.2.1",
    "typescript": "^3.1.6",
    "vo": "4.0.2",
    "webpack": "2.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
